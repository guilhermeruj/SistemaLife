<div class="container-fluid">
  <div class="col-lg-12 d-flex align-items-stretch">
    <div class="card w-100">
      <div class="card-body p-4">
        <h5 class="card-title fw-semibold mb-4">
          Detalhes dos serviço
        </h5>
        <div class="table-responsive">
          <table class="table text-nowrap mb-0 align-middle">
            <thead class="text-dark fs-4 bordar-linha">
              <tr>
                <th class="border-bottom-0">
                  <h6 class="fw-semibold mb-0">Codigo</h6>
                </th>
                </th><th class="border-bottom-0">
                  <h6 class="fw-semibold mb-0 ">Nome</h6>
                </th>
                <th class="border-bottom-0">
                  <h6 class="fw-semibold mb-0">Telefone</h6>
                </th>
                <th class="border-bottom-0">
                  <h6 class="fw-semibold mb-0">Email</h6>
                </th>  
                <th class="border-bottom-0">
                  <h6 class="fw-semibold mb-0">Edição</h6>
                </th>
              </tr>
            </thead>
            <tbody>
              {{#each clientes}}
                <tr>
                  <td class="border-bottom-0">
                    <h6 class="fw-semibold mb-0">{{this.id}}</h6>
                  </td>
                  <td class="border-bottom-0">
                    <h6 class="fw-semibold mb-1 maiusculo">{{this.name}}</h6>
                  </td>
                  <td class="border-bottom-0">
                    <h6 class="fw-semibold mb-1">{{this.tel}}</h6>
                  </td>
                  <td class="border-bottom-0">
                    <p class="mb-0 fw-normal">{{this.email}}</p>
                  </td>
                  <td class="border-bottom-0">
                    <a href="/perfilcliente/{{this.id}}"> <i class="bi bi-card-list icones"></i></a>
                    <a href="/editcliente/{{this.id}}"><i class="bi bi-pencil-square icones"></i></a>
                   <a href="/deletecliente/{{this.id}}"><i class="bi bi-trash icones"></i></a>
                  </td>
                </tr>
              {{/each}}
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>